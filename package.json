    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "typescript": "^5.0.2",
    "vite": "^4.3.2"
  },
  "lint-staged": {
    "src/**/*.{js,html.css}": "prettier --write",
    "src/*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
