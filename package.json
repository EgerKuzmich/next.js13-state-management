    "dayjs": "^1.11.7",
    "grommet": "^2.32.1",
    "grommet-icons": "^4.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.3.0",
    "react-responsive": "^9.0.2",
    "react-router-dom": "^6.11.2",
    "react-use": "^17.4.0",
    "styled-components": "^5.3.11",
    "vite-tsconfig-paths": "^4.2.0",
    "zustand": "^4.3.8"
  },
  "devDependencies": {
    "@types/node": "^20.2.5",
